<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :goal %>
    <%= f.association :user %>
    <%= f.association :page %>
    <%= f.input :public %>
    <%= f.association :category %>
    <%= f.association :phase %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
